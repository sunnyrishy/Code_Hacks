#include<bits/stdc++.h>
using namespace std;
using ll = long long;

int main () {
    ll n;
    cin>>n;
    if(n <= 2){
        cout<<n-1<<endl;
        return 0;
    }
    vector<ll> fib(n);
    fib[0] = 0;
    fib[1] = 1;
    for(ll i = 2; i < n; i++){
        fib[i] = fib[i-1] + fib[i-2];
    }
    cout<<fib[n-1]<<endl;
    return 0;

}
